  <ItemGroup>
    <PackageReference Include="Exceptionless" Version="4.4.0" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Kugar.Core.NetCore" Version="1.2.104" />
  </ItemGroup>

</Project>
